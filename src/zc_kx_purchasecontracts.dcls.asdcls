@EndUserText.label: 'DCL for ZC_KX_PurchaseContracts'
@MappingRole: true
define role ZC_KX_PURCHASECONTRACTS {
    grant
        select
            on
                ZC_KX_PurchaseContracts
                    where
                        inheriting conditions from entity ZI_PurchaseContracts;
                        
}